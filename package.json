    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@el3um4s/rollup-plugin-terser": "^1.0.2",
        "@rollup/plugin-eslint": "^9.0.1",
        "@types/jest": "^29.2.0",
        "babel-jest": "^29.2.1",
        "cssnano": "^5.1.13",
        "eslint": "^8.26.0",
        "fast-glob": "^3.2.12",
        "jest": "^29.2.1",
